*, input, :before, :after {
  -webkit-box-sizing:border-box;
  -moz-box-sizing:border-box;
  box-sizing:border-box;
}

// ==============================================================================
// #Breakpoint and Grid Variables
// ==============================================================================
$viewportIncrement: 1px;

$small: 480px;
$medium: 768px;
$large: 769px;

$postSmall: $small + $viewportIncrement;
$preMedium: $medium - $viewportIncrement;
$preLarge: $large - $viewportIncrement;

/* The following are dependencies of csswizardry grid */
$breakpoints: (
  'small' '(max-width: #{$small})',
  'medium' '(min-width: #{$postSmall}) and (max-width: #{$medium})',
  'medium-down' '(max-width: #{$medium})',
  'large' '(min-width: #{$large})'
);
$breakpoint-has-widths: ('small', 'medium', 'medium-down', 'large');
$breakpoint-has-push:  ('small', 'medium', 'medium-down', 'large');
$breakpoint-has-pull:  ('small', 'medium', 'medium-down', 'large');

// ==============================================================================
// #Custom Variables - Ryan Krieg
// ==============================================================================
$orange: #d57a5e;
$brown: rgba(23,23,23,0.8);
$light-grey: #9b9c9b;

// ==============================================================================
// #General Variables
// ==============================================================================

/* Button colors */
$colorBtnPrimary: $orange; // boring grey
$colorBtnPrimaryHover: darken($colorBtnPrimary, 10%);
$colorBtnPrimaryActive: darken($colorBtnPrimaryHover, 10%);

$colorBtnSecondary: $brown;
$colorBtnSecondaryHover: darken($colorBtnSecondary, 10%);
$colorBtnSecondaryActive: darken($colorBtnSecondaryHover, 10%);

/* Text link colors */
$colorLink: $orange;
$colorLinkHover: darken($orange, 10%);

/* Text colors */
$colorBody: #fff;
$colorText: #fff;
$colorHeaders: $orange;
$colorTextGrey: $light-grey;

/* Border colors */
$borderColor: #fff;

/* Nav and dropdown link background */
$colorNav: $brown;

/* Footer */
$colorFooterBg: $light-grey;

/* Helper colors */
$disabledGrey: #f6f6f6;
$disabledBorder: darken($disabledGrey, 25%);
$errorRed: #dc0000;
$errorRedBg: lighten($errorRed, 55%);
$successGreen: #0a942a;

/* Width variables */
$siteWidth: 1024px;
$gutter: 24px;
$gridGutter: 24px; // can be a %, but nested grids will have smaller margins because of it

/* Radius */
$radius: 3px;

/* Typography */
$headerFontStack: "liberatorregular", "aniversregular", "HelveticaNeue", "Helvetica Neue", Helvetica, Arial, sans-serif;
$bodyFontStack: "aniversregular", "HelveticaNeue", "Helvetica Neue", Helvetica, Arial, sans-serif;


// ==============================================================================
// #Sass Mixins
// ==============================================================================
.clearfix {
  &:after {
    content: "";
    display: table;
    clear: both; }
  *zoom: 1;
}
@mixin clearfix() {
  @extend .clearfix;
}

@mixin box-shadow($shadow) {
  -webkit-box-shadow: #{$shadow};
  -moz-box-shadow:    #{$shadow};
  box-shadow:         #{$shadow};
}

@mixin transition($transition:0.1s all) {
  -webkit-transition: #{$transition};
  -moz-transition: #{$transition};
  -o-transition: #{$transition};
  transition: #{$transition};
}

@mixin gradient($from, $to, $fallback) {
  background: $fallback;
  background: -moz-linear-gradient(top, $from 0%, $to 100%);
  background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,$from), color-stop(100%,$to));
  background: -webkit-linear-gradient(top, $from 0%,$to 100%);
  background: -o-linear-gradient(top, $from 0%,$to 100%);
  background: -ms-linear-gradient(top, $from 0%,$to 100%);
  background: linear-gradient(top bottom, $from 0%,$to 100%);
}

/*
  Dependency-free breakpoint mixin
  http://blog.grayghostvisuals.com/sass/sass-media-query-mixin/
*/
$min: min-width;
$max: max-width;
@mixin at-query($constraint, $viewport1, $viewport2:null) {

  @if $constraint == $min {
    @media screen and ($min: $viewport1) {
      @content;
    }
  } @else if $constraint == $max {
    @media screen and ($max: $viewport1) {
      @content;
    }
  } @else {
    @media screen and ($min: $viewport1) and ($max: $viewport2) {
      @content;
    }
  }

}

